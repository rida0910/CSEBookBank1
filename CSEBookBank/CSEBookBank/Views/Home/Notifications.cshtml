@model IEnumerable<CSEBookBank.Models.Notification>

@{
    ViewBag.Title = "Notifications";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Notifications</h2>

@foreach (var item in Model)
{ 
    <div class="alert alert-info">

        
            @Html.DisplayFor(modelItem => item.NotMessage)
            
    </div>
    <hr />
}